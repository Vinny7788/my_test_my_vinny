using Microsoft.Win32;
using System;
using System.ComponentModel;
using System.Globalization;
using System.Runtime.InteropServices;

namespace School
{
    internal class Program
    {
        static void Main(string[] args)
        {
            bool notMultiple;
            Random rand = new Random();
            int sumOfNumbers = 0;
            int rundomNumber = new Random().Next(1, 101);
            for (int i = 0; i < rundomNumber; i++)
            {
                notMultiple = Convert.ToBoolean(i % 3) || Convert.ToBoolean(i % 5);
                if (notMultiple == false)
                { sumOfNumbers += i; }
            }
                Console.WriteLine(sumOfNumbers += rundomNumber);
        }

    }
}
