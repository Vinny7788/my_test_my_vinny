using Microsoft.Win32;
using System;
using System.ComponentModel;
using System.Globalization;
using System.Runtime.InteropServices;

namespace School
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int maxValue = 100;

            for (int i = 5; i < maxValue; i += 7)
            {
                Console.WriteLine(i);
            }
        }
    }
}
   
