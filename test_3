using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.InteropServices;
using System.Text;
using System.Threading;
using System.Threading.Tasks;

namespace School
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int pictures = 52;
            int picturesInOneRow = 3;
            int numbersOfRow;
            int extraPictures = pictures % picturesInOneRow;
            int filledRows = (pictures - extraPictures) / picturesInOneRow;
            Console.WriteLine("Filled rows: " + filledRows);
            Console.WriteLine("Extra pictures: " + extraPictures);
            }   
        }
    }
   
