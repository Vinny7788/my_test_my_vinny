using System;
using System.Collections.Generic;

namespace School
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string[] numbersArrayOne = { "1", "5", "4", "3", "8", "5", "4", "3", "2" };
            string[] numbersArrayTwo = { "1", "8", "8", "2", "9" };

            List<string> mainArray = new List<string>();

            MergingArray(mainArray, numbersArrayOne);
            MergingArray(mainArray, numbersArrayTwo);

            for (int i = 0; i < mainArray.Count; i++)
            {
                Console.WriteLine(mainArray[i]);
            }

        }

        static void MergingArray(List<string> mainArray, string[] array)
        {

            for (int i = 0; i < array.Length; i++)
            {

                if (mainArray.Contains(array[i]) == false)
                {
                    mainArray.Add(array[i]);
                }

            }

        }
    }
}
